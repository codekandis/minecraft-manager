{
	"name": "codekandis/minecraft-manager",
	"description": "`codekandis/minecraft-manager` is a frontend application to help managing a minecraft world.",
	"keywords": [
		"php",
		"minecraft",
		"manager",
		"frontend",
		"application"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/minecraft-manager/issues",
		"docs": "https://github.com/codekandis/minecraft-manager/blob/master/README.md",
		"source": "https://github.com/codekandis/minecraft-manager/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/authentication": "^0",
		"codekandis/configurations": "^0",
		"codekandis/persistence": "^0",
		"codekandis/sentry-client": "^1",
		"codekandis/session": "^0",
		"codekandis/tiphy": "^0",
		"codekandis/tiphy-authentication-integration": "^0",
		"codekandis/tiphy-persistence-integration": "^0",
		"codekandis/tiphy-sentry-client-integration": "^0",
		"codekandis/tiphy-session-integration": "^0",
		"fortawesome/font-awesome": "^5"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\MinecraftManager\\": [
				"config/",
				"public/",
				"src/"
			],
			"CodeKandis\\MinecraftManager\\Configurations\\Plain\\": [
				"config/",
				"src/Configurations/Plain"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\MinecraftManager\\Tests\\": [
				"tests/"
			]
		}
	}
}
